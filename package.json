{
  "name": "bacon.react.atom-examples",
  "description": "Bacon+React+Atom Examples",
  "main": "public/app.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:css": "lessc src/*.less > public/bundle.css",
    "build:js": "browserify src/app.js -t babelify -g uglifyify > public/app.js",
    "watch": "npm run watch:js & npm run watch:css",
    "watch:css": "npm run build:css && watch-run -p 'src/*.less' 'npm run build:css'",
    "watch:js": "watchify src/app.js -v -t [ babelify --sourceMaps=inline ] -o public/app.js",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polytypic/bacon.react.atom-examples.git"
  },
  "keywords": [
    "bacon",
    "react",
    "atom",
    "reactive"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polytypic/bacon.react.atom-examples/issues"
  },
  "homepage": "https://github.com/polytypic/bacon.react.atom-examples#readme",
  "dependencies": {
    "bacon.atom": "^1.0.0",
    "bacon.react.html": "^1.1.0",
    "baconjs": "^0.7.83",
    "ramda": "^0.19.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.3.16",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.5.1",
    "less": "^2.5.3",
    "uglifyify": "^3.0.1",
    "watch-run": "^1.2.4",
    "watchify": "^3.4.0"
  }
}
